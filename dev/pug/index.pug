	//contact 
	section#contact.contact
		.container
			h1.title.text-center.mb-2.wow.slideInLeft( data-wow-duration="1s")  Contact 
			p.help-title.mb-5.wow.slideInLeft( data-wow-duration="1.5s") Fill this form to get in touch with us.
	.bg_dark-blue.pt-4.pb-4.wow.fadeInLeft(data-wow-duration="0.5s")
		.container
			.row
				.col-md-6
					form.contactForm.needs-validation(novalidate)
	
						.form-group
							label.sr-only Full name:
							.input-group
								.input-group-prepend.wow.fadeInLeft(data-wow-duration="2s" )
									.input-group-text.bg-secondary
										+icon('user-solid','form-icon form-icon_user')
								input.form-control.wow.fadeInRight(data-wow-duration="1s" type="text" placeholder="Full name" required)
								.invalid-feedback Write your full name here.
						.form-group
							label.sr-only E-mail Address:
							.input-group
								.input-group-prepend.wow.fadeInLeft(data-wow-duration="2s" )
									.input-group-text.bg-secondary
										+icon('envelope-solid','form-icon')
								input.form-control.wow.fadeInRight(data-wow-duration="1s" type="email" placeholder="E-mail Address" required)
								.invalid-feedback Write your Email Address here.
						.form-group
							label.sr-only Phone number:
							.input-group
								.input-group-prepend.wow.fadeInLeft(data-wow-duration="2s" )
										.input-group-text.bg-secondary
											+icon('phone-solid','form-icon')
								input.form-control.wow.fadeInRight.phone(data-wow-duration="1s" type="tel" data-mask="+3 (000) 000 00 00" placeholder="+3 (___) ___ __ __" required)
								.invalid-feedback Write your Phone Number here.
						.form-group
							label.sr-only Your deliver:
					
							textarea.form-control.wow.fadeInRight(data-wow-duration="1s" placeholder="Message you want to deliver..." rows="4" required )
							.invalid-feedback Write your Deliver here.

						.form-group
							input.btn.btn-primary.text-white.w-100.wow.fadeInRight(data-wow-duration="1s" type="submit" value="Submit")

				.col-md-6
					#map.map.mb-3
					address.wow.fadeInRight(data-wow-duration="2s" itemprop="address" itemscope itemtype="http://data-vocabulary.org/Address").text-white.address
						span(itemprop="street-address") 272 Collin Street
						|, 
						span( itemprop="locality") Melbourne 3000
						.d-flex.align-items-center
							span.mr-3 Call |  
							span(itemprop="tel").text-primary.display-4.phoneNum.font-weight-bold 09005896
					


