extends template/main

block variables
	- var title = 'burgers.co'
	- var pageClass = 'index-page'

block content

	section#home.hero.d-flex.flex-column.flex-md-row
		.hero-1.w-100.wow.fadeInLeft(data-wow-duration="0.5s")
			.mycontainer.pr-md-0.mycontainer_1
				h1.display-4.text-primary.font.mb-2.wow.slideInLeft( data-wow-duration="1s") We create Burgers.
				p.wow.slideInLeft.text-white( data-wow-duration="1.5s") Beautiful hand crafted burgers with tangy sauce lettuce, onions, pickles and our specials
		.hero-2.w-100.wow.fadeInRight(data-wow-duration="0.5s")
			.mycontainer.pl-md-5.mycontainer_2
				h1.display-4.text-white.font.mb-2.wow.slideInRight( data-wow-duration="1s") Delicious
				p.wow.slideInRight(data-wow-duration="1.5s") Oh, yes as are made with hands and chef loves them to the core.
				.d-flex.align-items-center.wow.slideInRight(data-wow-delay="0.5s" data-wow-duration="1s")
					span.mr-3.font-weight-bold Are they really?
					button.btn.btn-outline-dark.show_me Show me 
						+icon('hamburger','hamburger_icon')
	// burgers
	section#burgers.burgers
		.container 
			.row
				.col-md-4.mb-4.mb-md-0.d-flex.justify-content-center.justify-content-md-start
					.burger.position-relative
						img.burger__img.burger__img_1.mb-2.mb-md-4.wow.flipInX(data-wow-duration="1.5s" src="static/images/content/burger_1.png")
						.burger__body
							h5.burger__name.mb-1.mb-md-2.wow.fadeInUp(data-wow-duration="1s" ) Beef Bondy
							p.burger__text.wow.fadeInUp(data-wow-duration="1s" ) Juicy hand cut beef with Mango sauce fries
							a.burger__btn.btn.bookBurg.btn-light.justify-content-between.stretched-link.wow.fadeInUp(data-wow-duration="1.5s" href="#")
								.d-flex
									img.bookBurg__icon(src="static/images/general/barbecco-icon.png").mr-2
									span.bookBurg__kilo.mr-2 345 KJ
								span.bookBurg__price $12.5


				.col-md-4.mb-4.mb-md-0
					.burger.position-relative
						img.burger__img.burger__img_2.mb-2.mb-md-4.wow.flipInX(data-wow-duration="1.5s"  src="static/images/content/burger_2.png")
						.burger__body
							h5.burger__name.mb-1.mb-md-2.wow.fadeInUp(data-wow-duration="1s" ) Chicken Mota
							p.burger__text.wow.fadeInUp(data-wow-duration="1s" ) Beautiful chicken breast with salsa sauce on top
							a.burger__btn.btn.bookBurg.btn-light.justify-content-between.stretched-link.wow.fadeInUp(data-wow-duration="1.5s" href="#")
								.d-flex
									img.bookBurg__icon(src="static/images/general/barbecco-icon.png").mr-2
									span.bookBurg__kilo.mr-2 345 KJ
								span.bookBurg__price $9.0


				.col-md-4.d-flex.justify-content-center.justify-content-md-end
					.burger.position-relative
						img.burger__img.burger__img_3.mb-2.mb-md-4.wow.flipInX(data-wow-duration="1.5s"  src="static/images/content/burger_3.png")
						.burger__body
							h5.burger__name.mb-1.mb-md-2.wow.fadeInUp(data-wow-duration="1s" ) Double Beef
							p.burger__text.wow.fadeInUp(data-wow-duration="1s" ) Yes! Double patty with  seculent sauce
							a.burger__btn.btn.bookBurg.btn-light.justify-content-between.stretched-link.wow.fadeInUp(data-wow-duration="1.5s" href="#")
								.d-flex
									img.bookBurg__icon(src="static/images/general/barbecco-icon.png").mr-2
									span.bookBurg__kilo.mr-2 345 KJ
								span.bookBurg__price $13.6
	// burgers end 

	// wait
	section#wait.wait.w-100.wow.fadeInLeft(data-wow-duration="0.5s" da)
		.container
			.row
				.col-md-7.col-lg-5.col-xl-8.d-flex.align-items-md-center.mb-5.mb-md-0
					h1.title.text-white.wow.slideInLeft(data-wow-duration="0.8s") Wait! But we also serve ...

				.col-md-5.col-lg-7.col-xl-4.d-flex.justify-content-md-end
					ul.price-list.list-unstyled.wow.slideInRight( data-wow-duration="0.8s")
						li.price-list__item.mb-3.mb-md-4
							span.price-list__food French Fries
							span.price-list__price $12
						li.price-list__item.mb-3.mb-md-4
							span.price-list__food Cold Drinks
							span.price-list__price $8
						li.price-list__item.mb-3.mb-md-4
							span.price-list__food Love Choclates
							span.price-list__price $25
